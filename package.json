{
  "name": "pennies-web-raw",
  "version": "0.0.1",
  "main": "src/index.js",
  "repository": {},
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server",
    "dev:mb": "mb --configfile test/mountebank-dev.ejs",
    "dev:stub": "API_HOST=\"'http://localhost:5555'\" run-p dev:mb dev",
    "dev:local": "API_HOST=\"'http://localhost:9001'\" webpack-dev-server",
    "dev:production": "API_HOST=\"'https://rampton.herokuapp.com'\" webpack-dev-server",
    "lint": "standard \"src/**/*.js\"",
    "test": "jest",
    "build": "webpack"
  },
  "precommit": [
    "lint",
    "test"
  ],
  "standard": {
    "globals": [
      "__API_HOST__",
      "jest",
      "describe",
      "expect",
      "it"
    ]
  },
  "jest": {
    "testMatch": [
      "**/*-test.js"
    ],
    "globals": {
      "__API_HOST__": "pop"
    },
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/test/jest-file-mock.js"
    }
  },
  "dependencies": {
    "bootstrap": "4.0.0-alpha.6",
    "classnames": "^2.2.5",
    "firebase": "^3.7.0",
    "font-awesome": "^4.7.0",
    "load-script": "^1.0.0",
    "moment": "^2.17.1",
    "ramda": "^0.23.0",
    "react": "^15.4.2",
    "react-burger-menu": "^1.10.14",
    "react-dom": "^15.4.2",
    "react-masonry-component": "^5.0.3",
    "react-redux": "^5.0.3",
    "react-redux-form": "latest",
    "react-router": "^3.0.2",
    "react-select": "^1.0.0-rc.5",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.0",
    "redux-logger": "^2.8.2",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.2",
    "slugify": "^1.1.0",
    "webpack": "^2.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.26.2",
    "enzyme": "^2.8.2",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^20.0.4",
    "mountebank": "^1.11.0",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.2",
    "pre-commit": "^1.2.2",
    "react-addons-test-utils": "^15.5.1",
    "standard": "^9.0.2",
    "style-loader": "^0.13.2",
    "webpack-dev-server": "^2.4.1"
  }
}
